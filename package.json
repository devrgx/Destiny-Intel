    "discord.js": "^14.19.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
